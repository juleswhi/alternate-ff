{
    "manifest_version": 2,
    "name": "Alternate",
    "version": "1.0",

    "description": "Adds an alternate tab key",

    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "commands": {
        "toggle-feature": {
            "suggested_key": { "default": "Ctrl+6" },
            "description": "Send a 'toggle-feature' event to the extension"
        }
    }
}
