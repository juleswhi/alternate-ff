{
    "manifest_version": 2,
    "name": "Alternate",
    "version": "1.0",

    "description": "Adds an alternate tab keybind",

    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "commands": {
        "alternate": {
            "suggested_key": { "default": "Ctrl+6" },
            "description": "Sends an alternate command"
        }
    }
}
